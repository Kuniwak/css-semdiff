{
  "name": "css-semdiff",
  "version": "1.0.2",
  "description": "Semantic diff tool for CSS",
  "main": "index.js",
  "dependencies": {
    "chardet": "^0.1.0",
    "commander": "^2.8.1",
    "css": "^2.2.1",
    "dtsm": "^1.0.0",
    "es6-promise": "^3.0.2",
    "stream-to-promise": "^1.1.0",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "assert-polyfill": "0.0.0",
    "mocha": "^2.3.3",
    "npm-run-all": "^1.8.0",
    "rimraf": "^2.5.2",
    "tslint": "^3.8.1"
  },
  "scripts": {
    "prepublish": "npm-run-all lint typings build test",
    "typings": "dtsm install",
    "prebuild": "rimraf dist",
    "build:typescript": "tsc",
    "build": "npm-run-all prebuild build:typescript",
    "watch": "tsc --watch",
    "pretest": "npm run build",
    "test": "mocha --opts mocha.opts dist/tests",
    "lint": "tslint src/**/*.ts"
